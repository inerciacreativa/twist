{
	"name": "inerciacreativa/twist",
	"description": "Twist theme for WordPress",
	"type": "wordpress-theme",
	"homepage": "https://inerciacreativa.com/twist/",
	"keywords": [
		"theme",
		"wordpress",
		"twig"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "JosÃ© Cuesta",
			"email": "jose.cuesta@inerciacreativa.com",
			"homepage": "https://inerciacreativa.com/",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.4",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-simplexml": "*",
		"composer/installers": "^2.0",
		"monolog/monolog": "^2.3",
		"pimple/pimple": "^3.0",
		"psr/log": "^1.0",
		"twig/twig": "^3.0"
	},
	"require-dev": {
		"kint-php/kint": "^3.3",
		"phpunit/phpunit": "^8.5",
		"roave/security-advisories": "dev-master"
	},
	"config": {
		"vendor-dir": "vendor"
	},
	"autoload": {
		"psr-4": {
			"Twist\\": "engine/"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"Twist\\Tests\\": "tests/unit/"
		}
	}
}
